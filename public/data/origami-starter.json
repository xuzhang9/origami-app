  {
    "id": "origami-crane",
    "name": "Paper Crane",
    "difficulty": "hard",
    "category": "animals",
    "thumbnailUrl": "/images/crane-thumb.svg",
    "sourceUrl": "https://origami-resource-center.com/crane",
    "steps": [
      {
        "stepNumber": 1,
        "instruction": "Start with a square piece of paper. Fold in half both ways to create creases, then unfold.",
        "imageUrl": "/images/crane-step1.svg"
      },
      {
        "stepNumber": 2,
        "instruction": "Fold diagonally both ways, then unfold. You should have a star pattern of creases.",
        "imageUrl": "/images/crane-step2.svg"
      },
      {
        "stepNumber": 3,
        "instruction": "Bring all four corners together to create a small square (this is called a square base).",
        "imageUrl": "/images/crane-step3.svg"
      },
      {
        "stepNumber": 4,
        "instruction": "Fold the sides to the center, then unfold. Lift the bottom point up and press flat (petal fold).",
        "imageUrl": "/images/crane-step4.svg"
      },
      {
        "stepNumber": 5,
        "instruction": "Repeat on all four sides. Fold the wings down and pull out the head and tail.",
        "imageUrl": "/images/crane-step5.svg"
      },
      {
        "stepNumber": 6,
        "instruction": "Fold down the tip of the head. Gently pull the wings apart. Your crane is complete!",
        "imageUrl": "/images/crane-step6.svg"
      }
    ]
  },
  {
    "id": "origami-boat",
    "name": "Sailing Boat",

  {
    "id": "origami-star",
    "name": "Lucky Star",
    "difficulty": "medium",
    "category": "decorations",
    "thumbnailUrl": "/images/star-thumb.svg",
    "sourceUrl": "https://origami-resource-center.com/star",
    "steps": [
      {
        "stepNumber": 1,
        "instruction": "Cut a long strip of paper about 1cm wide and 30cm long.",
        "imageUrl": "/images/star-step1.svg"
      },
      {
        "stepNumber": 2,
        "instruction": "Make a loop at one end and thread the other end through to create a knot. Gently tighten into a pentagon shape.",
        "imageUrl": "/images/star-step2.svg"
      },
      {
        "stepNumber": 3,
        "instruction": "Wrap the long end around each edge of the pentagon, following the shape.",
        "imageUrl": "/images/star-step3.svg"
      },
      {
        "stepNumber": 4,
        "instruction": "When you reach the end, tuck the remaining paper into the folds.",
        "imageUrl": "/images/star-step4.svg"
      },
      {
        "stepNumber": 5,
        "instruction": "Gently pinch each edge of the pentagon inward to create a puffy 3D star!",
        "imageUrl": "/images/star-step5.svg"
      }
    ]
  }
